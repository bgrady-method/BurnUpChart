# Environment variables for Scope vs Completed Jira Analysis
# Copy this file to .env and fill in your actual values

# Jira Configuration (optional - for live MCP integration)
JIRA_URL=https://your-instance.atlassian.net
JIRA_USERNAME=your.email@company.com
JIRA_API_TOKEN=your-jira-api-token-here

# MCP Server Configuration (optional)
MCP_ATLASSIAN_SERVER=github.com/pashpashpash/mcp-atlassian

# Application Configuration
DEFAULT_JQL=Epic Link = PL-54667
DEFAULT_TIMEZONE=America/Toronto
DEFAULT_START_DATE=2025-09-09
DEFAULT_END_DATE=2025-11-11

# Default Exclusions (comma-separated issue keys)
DEFAULT_EXCLUDE_KEYS=PL-56887,PL-56143

# Authentication
APP_PASSWORD=your_secure_password_here

# Debug/Development
DEBUG=false
LOG_LEVEL=INFO
